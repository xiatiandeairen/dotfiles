				<key>script</key>
				<string>const richTextStyle = `
  &lt;style&gt;
    body { font-family: sans-serif; font-size: 14; }
    code { font-family: monospace; background-color: #eee; }
    p, ul, ol { margin: 0; }
  &lt;/style&gt;
`

function run(argv) {
  const html = `${richTextStyle}${argv[0]}`
    .replaceAll(/&lt;img.*?&gt;/g, "") // Strip images
    .replaceAll("&lt;/p&gt;", "&lt;/p&gt;&lt;br&gt;") // Enforce line breaks after paragraphs
    .replaceAll("&lt;/pre&gt;", "&lt;/pre&gt;&lt;br&gt;") // Enforce line breaks after code blocks

  const task = $.NSTask.alloc.init
  const stdin = $.NSPipe.pipe
  const stdout = $.NSPipe.pipe

  task.executableURL = $.NSURL.fileURLWithPath("/usr/bin/textutil")
  task.arguments = ["-inputencoding", "utf8", "-convert", "rtf", "-stdin", "-stdout", "-format", "html"]
  task.standardInput = stdin
  task.standardOutput = stdout
  task.launchAndReturnError(false)

  const dataIn = $(html).dataUsingEncoding($.NSUTF8StringEncoding)
  stdin.fileHandleForWriting.writeDataError(dataIn, false)
  stdin.fileHandleForWriting.closeAndReturnError(false)

  const dataOut = stdout.fileHandleForReading.readDataToEndOfFileAndReturnError(false)
  const stringOut = $.NSString.alloc.initWithDataEncoding(dataOut, $.NSUTF8StringEncoding).js

  return stringOut
}</string>
