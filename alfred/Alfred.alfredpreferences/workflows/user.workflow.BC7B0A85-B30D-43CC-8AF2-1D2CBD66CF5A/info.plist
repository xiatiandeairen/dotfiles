				<string>String.prototype.bbSub = function(htmlTag, bbcodeTag) {
  return this
    .replaceAll(`&lt;${htmlTag}&gt;`, `[${bbcodeTag}]`)
    .replaceAll(`&lt;/${htmlTag}&gt;`, `[/${bbcodeTag}]`)
}

function run(argv) {
  return argv[0]
    .replaceAll("&lt;p&gt;", "").replaceAll("&lt;/p&gt;", "\n")
    .bbSub("strong", "b")
    .bbSub("em", "i")
    .bbSub("strikethrough", "s")
    .bbSub("underline", "u")
    .replaceAll(/&lt;a href="(.*?)"&gt;/g, "[url='$1']").replaceAll("&lt;/a&gt;", "[/url]")
    .replaceAll("&lt;ol&gt;", "[list=1]").replaceAll("&lt;/ol&gt;", "[/list]")
    .replaceAll(/&lt;img.*?src=['"](.+?)['"].*?&gt;/g, "[img]$1[/img]")
    .replaceAll(/&lt;\/?figure&gt;/g, "")
    .replaceAll(/&lt;figcaption&gt;.*&lt;\/figcaption&gt;/g, "")
    .bbSub("ul", "list")
    .bbSub("li", "*")
    .bbSub("blockquote", "quote")
    .replaceAll(/&lt;pre&gt;&lt;code.*?&gt;/g, "[code]").replaceAll("&lt;/code&gt;&lt;/pre&gt;", "[/code]")
    .replaceAll("&lt;code&gt;", "[background=#eee][font=courier,monospace]").replaceAll("&lt;/code&gt;", "[/font][/background]")
    .replaceAll(/&lt;h1.*?&gt;/g, "\n\n[b][size=7]").replaceAll("&lt;/h1&gt;", "[/size][/b]")
    .replaceAll(/&lt;h2.*?&gt;/g, "\n\n[b][size=6]").replaceAll("&lt;/h2&gt;", "[/size][/b]")
    .replaceAll(/&lt;h3.*?&gt;/g, "\n\n[b][size=5]").replaceAll("&lt;/h3&gt;", "[/size][/b]")
    .replaceAll(/&lt;h4.*?&gt;/g, "\n\n[b][size=4]").replaceAll("&lt;/h4&gt;", "[/size][/b]")
    .bbSub("sup", "sup")
    .bbSub("sub", "sub")
    .replaceAll("&lt;hr /&gt;", "[hr]")
    .replaceAll(/\n*\[list(=1)?\]$/gm, "[list$1]") // Adjustments to list outward spacing in IPBoard
    .replaceAll(/\[\/list\]\n*/gm, "[/list]")
    .replaceAll(/^\[quote\]\n*/gm, "[quote]") // Adjustments to qupte spacing in IPBoard
    .replaceAll(/\n*\[\/quote\]$/gm, "[/quote]")
    .replaceAll(/^$/gm, "\n") // Bypass empty line collapsing done by IPBoard
}</string>
