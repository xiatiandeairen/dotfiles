# stow
export STOW_DIR=${DOTFILES}

#brew
eval "$($(which brew) shellenv)"

# the fuck
eval "$(thefuck --alias)"

#zoxide
export _ZO_DATA_DIR="${XDG_DATA_HOME}/zoxide"
export _ZO_ECHO=1
export _ZO_RESOLVE_SYMLINKS=1
export _ZO_MAXAGE=10000
export _ZO_EXCLUDE_DIRS="$HOME/private/*"
eval "$(zoxide init zsh)"
